{
    "manifest_version": 3,
    "name": "DET Practice Tools",
    "version": "1.0.0",
    "description": "Master the Duolingo English Test with daily vocabulary flashcards, exam countdown, and useful tools.",
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {},
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}